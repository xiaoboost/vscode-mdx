{
  "name": "@mdx/parser",
  "version": "1.0.0",
  "description": "mdx parser",
  "license": "MIT",
  "source": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/index.js",
  "keywords": [
    "mdx",
    "parser"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "watch": "pnpm run build -- -w",
    "test": "ava -v",
    "test:update": "ava -u",
    "lint": "eslint --quiet --no-error-on-unmatched-pattern --config ../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix --quiet --no-error-on-unmatched-pattern --config ../../.eslintrc.js src/**/*.{ts,tsx,js,jsx}"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "environmentVariables": {
      "TS_NODE_PROJECT": "tsconfig.json"
    },
    "files": [
      "tests/**/*.spec.ts"
    ]
  },
  "dependencies": {
    "@mdx/source-map": "workspace:*",
    "@mdx/utils": "workspace:*",
    "@xiao-ai/utils": "^1.4.3"
  },
  "devDependencies": {
    "@mdx/tsconfig": "workspace:*",
    "@types/mdx": "workspace:*",
    "@types/node": "^16.11.7",
    "ava": "^3.15.0",
    "eslint": "^8.2.0",
    "remark": "^14.0.1",
    "remark-mdx": "^1.6.22",
    "remark-parse": "^10.0.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "unified": "^10.1.0"
  }
}
