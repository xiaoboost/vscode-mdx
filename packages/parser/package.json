{
  "name": "@xvue/parser",
  "version": "0.0.1",
  "description": "",
  "author": "xiaoboost",
  "license": "ISC",
  "main": "dist/cjs",
  "module": "dist/es",
  "types": "dist/types",
  "keywords": [
    "vue",
    "parser"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "dev": "tsc -p ./tsconfig.build.json --outDir ./dev --declarationDir ./dev -w",
    "build:cjs": "tsc -p ./tsconfig.build.json --outDir ./dist/cjs --module commonjs",
    "build:es": "tsc -p ./tsconfig.build.json --outDir ./dist/es  --module esnext",
    "build": "pnpm run clear && pnpm run build:cjs && pnpm run build:es",
    "prepare": "pnpm run build"
  },
  "ava": {
    "serial": true,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only.js",
      "tsconfig-paths/register.js"
    ],
    "environmentVariables": {
      "TS_NODE_PROJECT": "tsconfig.test.json"
    },
    "files": [
      "tests/**/*.spec.ts"
    ]
  },
  "dependencies": {
    "@xvue/utils": "workspace:*",
    "line-column": "^1.0.2",
    "path-browserify": "^1.0.1",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@types/line-column": "^1.0.0",
    "ava": "^3.13.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  }
}
